{
    "files.exclude": {
        "*.egg-info": true,
        "**/__pycache__/": true,
        "**/.cache/": true,
        "**/.coverage": true,
        "**/.pytest_cache/": true,
        "**/.venv/": true,
        "**/dist/": true,
        "**/node_modules/": true,
        "setup.py": true
    },
    "python.defaultInterpreterPath": ".venv/bin/python",
    "html.format.wrapAttributes": "force-aligned",
    "editor.formatOnSave": true,
    "cSpell.words": [
        "aiohttp",
        "ballotpreview",
        "bugsnag",
        "cimg",
        "fbclid",
        "htmlcov",
        "maxsplit",
        "nonvote",
        "pyproject",
        "Rosalynn",
        "textbbox",
        "verchew",
        "werkzeug"
    ]
}
